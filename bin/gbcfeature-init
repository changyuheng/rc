if [ ! "$1" ]; then
	exit 1
fi

git status -s | grep '[MA] .*/'"$1"'/' | sed "s#[MA]\s\+\(.*/\)\($1\)\(/.*\)#cp -a \"\1_code_base\3\" \"\1/$1/\3\"#" | sh

