#!/system/bin/sh
#!/usr/bin/env sh

. /system/bin/.gbrc

for i in `ls /`; do
	i="/$i"
	skip="0"
	for j in `not_rootfs` "/mnt" "/sdcard"; do
		case "$j" in
			${i}*)
				skip="1"
				continue
				;;
			'/'${i}*)
				skip="1"
				continue
				;;
		esac
	done
	case "$skip" in
		"0")
			lsr "$i"
			;;
		*)
			#echo "skip: $i"
			;;
	esac
done

